{
    "name": "angular-bootstrap-material-starter",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "ng": "ng",
        "preinstall": "rm -rf node_modules",
        "start": "ng serve",
        "build": "ng build",
        "build:prod": "ng build --prod",
        "test": "ng test --code-coverage --browsers ChromeHeadless --watch=false",
        "test:dev": "ng test --code-coverage",
        "lint": "ng lint angular-bootstrap-material-starter",
        "lint:fix": "ng lint angular-bootstrap-material-starter --fix",
        "e2e": "ng e2e",
        "prettier": "pretty-quick --staged",
        "gh-pages": "ng build --prod --base-href /angular-bootstrap-material-starter/ && npx angular-cli-ghpages"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run prettier",
            "pre-push": "npm run lint && npm run test && npm run gh-pages"
        }
    },
    "dependencies": {
        "@angular/animations": "10.0.7",
        "@angular/cdk": "9.2.4",
        "@angular/common": "10.0.7",
        "@angular/compiler": "10.0.7",
        "@angular/core": "10.0.7",
        "@angular/flex-layout": "10.0.0-beta.32",
        "@angular/forms": "10.0.7",
        "@angular/material": "9.2.4",
        "@angular/platform-browser": "10.0.7",
        "@angular/platform-browser-dynamic": "10.0.7",
        "@angular/router": "10.0.7",
        "bootstrap": "4.5.0",
        "rxjs": "6.6.2",
        "tslib": "^2.0.0",
        "zone.js": "0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.1000.5",
        "@angular/cli": "10.0.5",
        "@angular/compiler-cli": "10.0.7",
        "@angular/language-service": "10.0.7",
        "@types/jasmine": "3.5.12",
        "@types/jasminewd2": "2.0.8",
        "@types/node": "12.12.53",
        "angular-cli-ghpages": "0.6.2",
        "codelyzer": "^6.0.0",
        "husky": "3.1.0",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~3.3.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "prettier": "1.19.1",
        "pretty-quick": "2.0.1",
        "protractor": "~7.0.0",
        "ts-node": "8.10.2",
        "tslint": "~6.1.0",
        "tslint-config-airbnb": "5.11.2",
        "tslint-config-prettier": "1.18.0",
        "typescript": "3.9.7"
    }
}
