{
    "name": "angular-bootstrap-material-starter",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "ng": "ng",
        "preinstall": "rm -rf node_modules",
        "start": "ng serve",
        "build": "ng build",
        "build:prod": "ng build --prod",
        "test": "ng test --code-coverage --browsers ChromeHeadless --watch=false",
        "test:dev": "ng test --code-coverage",
        "lint": "ng lint angular-bootstrap-material-starter",
        "lint:fix": "ng lint angular-bootstrap-material-starter --fix",
        "e2e": "ng e2e",
        "prettier": "pretty-quick --staged",
        "gh-pages": "ng build --prod --base-href /angular-bootstrap-material-starter/ && npx angular-cli-ghpages"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run prettier",
            "pre-push": "npm run lint && npm run test && npm run gh-pages"
        }
    },
    "dependencies": {
        "@angular/animations": "18.2.9",
        "@angular/cdk": "10.2.7",
        "@angular/common": "18.2.9",
        "@angular/compiler": "18.2.9",
        "@angular/core": "18.2.9",
        "@angular/flex-layout": "10.0.0-beta.32",
        "@angular/forms": "18.2.9",
        "@angular/material": "10.2.7",
        "@angular/platform-browser": "18.2.9",
        "@angular/platform-browser-dynamic": "18.2.9",
        "@angular/router": "18.2.9",
        "bootstrap": "4.6.2",
        "rxjs": "6.6.7",
        "tslib": "2.6.2",
        "zone.js": "0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.1100.7",
        "@angular/cli": "10.2.1",
        "@angular/compiler-cli": "18.2.9",
        "@angular/language-service": "18.2.9",
        "@types/jasmine": "3.10.18",
        "@types/jasminewd2": "2.0.13",
        "@types/node": "12.20.55",
        "angular-cli-ghpages": "0.6.2",
        "codelyzer": "6.0.2",
        "husky": "4.3.8",
        "jasmine-core": "3.99.1",
        "jasmine-spec-reporter": "5.0.2",
        "karma": "6.4.3",
        "karma-chrome-launcher": "3.2.0",
        "karma-coverage-istanbul-reporter": "3.0.3",
        "karma-jasmine": "4.0.2",
        "karma-jasmine-html-reporter": "1.7.0",
        "prettier": "2.8.8",
        "pretty-quick": "2.0.2",
        "protractor": "7.0.0",
        "ts-node": "8.10.2",
        "tslint": "6.1.3",
        "tslint-config-airbnb": "5.11.2",
        "tslint-config-prettier": "1.18.0",
        "typescript": "3.9.10"
    }
}
